    ##################################################################
    # vBump execution
    ##################################################################        
    :dcSet:
    {
        :name: vBump
        :table:
        {
            :format:
            { :dcEvent:     :pinList:   :dcSetup:   :type: }
            { forceLow       VDD,VDDQ    vBump,1   dcscale }
            { forceHigh      VDD,VDDQ    vBump,2   dcscale }
            { forceDefault   VDD,VDDQ    vBump,3   dcscale }
        }
    }

    ##################################################################
    # noise injection
    ##################################################################        
    :dcSet:
    {
        :name: PowerModulation
        :table:
        {
            :format:
            { :dcEvent:     :pinList:   :dcSetup:        :type:  }
            { sine000       VDD,VDDQ    modulation,0     dcscale }
            { sine030       VDD,VDDQ    modulation,30    dcscale }
            { sine060       VDD,VDDQ    modulation,60    dcscale }
            { sine090       VDD,VDDQ    modulation,90    dcscale }
            { sine210       VDD,VDDQ    modulation,210   dcscale }
            { sine240       VDD,VDDQ    modulation,240   dcscale }
            { sine270       VDD,VDDQ    modulation,270   dcscale }
            { forceDefault  VDD,VDDQ    modulation,99    dcscale }
        }
        
        :dcSequence:
        {
          :name: sineWave
          :interface: { sampleRate }
          :period: "12 * sampleRate"
          :table:
          {
              :format:
              { :dcEvent:     :offset:         }
              { sine000      " 0 * sampleRate" }
              { sine030      " 1 * sampleRate" }
              { sine060      " 2 * sampleRate" }
              { sine090      " 3 * sampleRate" }
              { sine060      " 4 * sampleRate" }
              { sine030      " 5 * sampleRate" }
              { sine000      " 6 * sampleRate" }
              { sine210      " 7 * sampleRate" }
              { sine240      " 8 * sampleRate" }
              { sine270      " 9 * sampleRate" }
              { sine240      "10 * sampleRate" }
              { sine210      "11 * sampleRate" }
          }
        }
    }

    ##################################################################
    # IDD measurements
    ##################################################################        
    :dcSet:
    {
        :name: IDD2N
        :table:
        {
            :format:
            { :dcEvent:     :pinList:   :dcSetup:  :type: }
            { setRange       VDD        IDD2N,0     dcscale }
            { measure        VDD        IDD2N,1     dcscale }
        }
    }       
    :dcSet:
    {
        :name: IDD2P0
        :table:
        {
            :format:
            { :dcEvent:     :pinList:   :dcSetup:  :type: }
            { setRange       VDD        IDD2P0,0     dcscale }
            { measure        VDD        IDD2P0,1     dcscale }
        }
    }       
    :dcSet:
    {
        :name: IDD4R
        :table:
        {
            :format:
            { :dcEvent:     :pinList:   :dcSetup:  :type: }
            { setRange       VDD,VDDQ   IDD4R,0     dcscale }
            { measure        VDD,VDDQ   IDD4R,1     dcscale }
        }
    }       
}
