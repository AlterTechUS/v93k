        :name: RTT40
        :table:
        {
            :format:
            { :dcEvent:     :pinList:    :dcSetup:        :type: }
#if ${DataBusWidth} == 8  
            { RTT40pu       DQS.000,DQS.001,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007      "LBL DC_MEAS_RTT40_PU"     pmu }
            { RTT40pd       DQS.000,DQS.001,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007      "LBL DC_MEAS_RTT40_PD"     pmu }
