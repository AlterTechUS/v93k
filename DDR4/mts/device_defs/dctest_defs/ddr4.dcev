:dcTest:
{
    ##################################################################
    # RON measurements
    ##################################################################        
    :dcSet:
    {
        :name: RON34
        :table:
        {
            :format:
            { :dcEvent:     :pinList:    :dcSetup:        :type: }
#if ${DataBusWidth} == 8  
            { RON34      DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007      "LBL DC_MEAS_RON34"   pmu }
#elif ${DataBusWidth} == 16
            { RON34      DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007,DQ.008,DQ.009,DQ.010,DQ.011,DQ.012,DQ.013,DQ.014,DQ.015     "LBL DC_MEAS_RON34"   pmu }
