    {
        :name: RTT60
        :table:
        {
            :format:
            { :dcEvent:     :pinList:    :dcSetup:        :type: }
#if ${DataBusWidth} == 8  
            { RTT60pu      DQS.000,DQS.001,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007      "LBL DC_MEAS_RTT60_PU"     pmu }
            { RTT60pd      DQS.000,DQS.001,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007      "LBL DC_MEAS_RTT60_PD"     pmu }
#elif ${DataBusWidth} == 16
            { RTT60pu      DQS.000,DQS.001,DQS.002,DQS.003,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007,DQ.008,DQ.009,DQ.010,DQ.011,DQ.012,DQ.013,DQ.014,DQ.015     "LBL DC_MEAS_RTT60_PU"     pmu }
            { RTT60pd      DQS.000,DQS.001,DQS.002,DQS.003,DQ.000,DQ.001,DQ.002,DQ.003,DQ.004,DQ.005,DQ.006,DQ.007,DQ.008,DQ.009,DQ.010,DQ.011,DQ.012,DQ.013,DQ.014,DQ.015     "LBL DC_MEAS_RTT60_PD"     pmu }
